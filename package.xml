  <version>2.2.0</version>
  <description>MoveIt configuration built around Clearpath Configuration</description>

  <author email="lcamero@clearpathrobotics.com">Luis Camero</author>

  <maintainer email="lcamero@clearpathrobotics.com">Luis Camero</maintainer>

  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>moveit_planners_chomp</exec_depend>
  <exec_depend version_gte="2.1.0">clearpath_manipulators_description</exec_depend>
  <exec_depend>gripper_controllers</exec_depend>
  <exec_depend>moveit_configs_utils</exec_depend>
  <exec_depend>moveit_kinematics</exec_depend>
  <exec_depend>moveit_planners</exec_depend>
  <exec_depend>moveit_ros_move_group</exec_depend>
  <exec_depend>moveit_ros_warehouse</exec_depend>
  <exec_depend>moveit_simple_controller_manager</exec_depend>
  <exec_depend>position_controllers</exec_depend>
  <exec_depend>tf2_ros</exec_depend>
